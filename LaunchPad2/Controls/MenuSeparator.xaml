<UserControl x:Class="LaunchPad2.Controls.MenuSeparator"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal">
        <Border  BorderBrush="#222222" BorderThickness="1,0,0,0" Margin="3,3,0,3"/>
        <Border  BorderBrush="{StaticResource MenuDisabledBrushKey}" BorderThickness="1,0,0,0" Margin="0,3,3,3"/>
    </StackPanel>
</UserControl>
